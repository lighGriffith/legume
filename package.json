{
  "name": "express-react-redux-starter",
  "version": "1.1.0",
  "description": "Starter for Express, React, Redux, SCSS applications",
  "scripts": {
    "dev": "webpack-dev-server --watch --colors",
    "build": "rimraf dist && NODE_ENV=developpement webpack --colors",
    "start": "NODE_ENV=production PORT=8080 pm2 start ./server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack"
  },
  "keywords": [
    "ExpressJS",
    "ReactJS",
    "Redux",
    "React hot loader",
    "React Router",
    "SCSS",
    "Webpack Devevelopment configuration",
    "Webpack Production configuration",
    "Airbnb Eslint",
    "pm2",
    "mocha",
    "chai"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lighGriffith/legume"
  },
  "author": "lighGriffith",
  "license": "free",
